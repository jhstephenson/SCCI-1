<p id="notice"><%= notice %></p>

<h1>Listing Messages</h1>

<table>
  <thead>
    <tr>
      <th>From</th>
      <th>Of</th>
      <th>Phone</th>
      <th>Cell phone</th>
      <th>Message</th>
      <th>Telephoned</th>
      <th>Returned your call</th>
      <th>Please call</th>
      <th>Please call</th>
      <th>Will call again</th>
      <th>Came to see you</th>
      <th>Wants to see you</th>
      <th>Other</th>
      <th>Other text</th>
      <th>Boolean</th>
      <th>Signed</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @messages.each do |message| %>
      <tr>
        <td><%= message.from %></td>
        <td><%= message.of %></td>
        <td><%= message.phone %></td>
        <td><%= message.cell_phone %></td>
        <td><%= message.message %></td>
        <td><%= message.telephoned %></td>
        <td><%= message.returned_your_call %></td>
        <td><%= message.please_call %></td>
        <td><%= message.please_call %></td>
        <td><%= message.will_call_again %></td>
        <td><%= message.came_to_see_you %></td>
        <td><%= message.wants_to_see_you %></td>
        <td><%= message.other %></td>
        <td><%= message.other_text %></td>
        <td><%= message.boolean %></td>
        <td><%= message.signed %></td>
        <td><%= link_to 'Show', message %></td>
        <td><%= link_to 'Edit', edit_message_path(message) %></td>
        <td><%= link_to 'Destroy', message, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Message', new_message_path %>
